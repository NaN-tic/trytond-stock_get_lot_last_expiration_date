<tryton>
    <data>
        <!-- stock.lot -->
        <record model="ir.ui.view" id="lot_view_form">
            <field name="model">stock.lot</field>
            <field name="inherit" ref="stock_lot.lot_view_form"/>
            <field name="name">lot_form</field>
        </record>
        <record model="ir.ui.view" id="lot_view_tree">
            <field name="model">stock.lot</field>
            <field name="inherit" ref="stock_lot.lot_view_tree"/>
            <field name="name">lot_list</field>
        </record>

        <record model="res.group" id="group_stock_lot_warehouse">
          <field name="name">Wharehouse (Stock Lot)</field>
        </record>
        <record model="res.group" id="group_stock_lot_td">
          <field name="name">Technical Direction (Stock lot)</field>
        </record>
        <record model="res.group" id="group_stock_lot_qd">
          <field name="name">Quality Department (Stock lot)</field>
        </record>
        <record model="res.group" id="group_stock_lot_admin">
          <field name="name">Admin (Stock lot)</field>
        </record>
        <record model="res.user-res.group" id="user_admin_group_stock_lot_warehouse">
          <field name="user" ref="res.user_admin"/>
          <field name="group" ref="group_stock_lot_warehouse"/>
        </record>
        <record model="res.user-res.group" id="user_admin_group_stock_lot_td">
          <field name="user" ref="res.user_admin"/>
          <field name="group" ref="group_stock_lot_td"/>
        </record>
        <record model="res.user-res.group" id="user_admin_group_stock_lot_qd">
          <field name="user" ref="res.user_admin"/>
          <field name="group" ref="group_stock_lot_qd"/>
        </record>
        <record model="res.user-res.group" id="user_admin_group_stock_lot_admin">
          <field name="user" ref="res.user_admin"/>
          <field name="group" ref="group_stock_lot_admin"/>
        </record>

        <record model="ir.model.access" id="stock_lot.access_lot_group_stock">
            <field name="model" search="[('model', '=', 'stock.lot')]"/>
            <field name="group" ref="stock.group_stock"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="True"/>
        </record>

        <record model="ir.model.access" id="access_lot_group_stock_lot_warehouse">
            <field name="model" search="[('model', '=', 'stock.lot')]"/>
            <field name="group" ref="group_stock_lot_warehouse"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <record model="ir.model.access" id="access_lot_group_stock_lot_td">
            <field name="model" search="[('model', '=', 'stock.lot')]"/>
            <field name="group" ref="group_stock_lot_td"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <record model="ir.model.access" id="access_lot_group_stock_lot_qd">
            <field name="model" search="[('model', '=', 'stock.lot')]"/>
            <field name="group" ref="group_stock_lot_qd"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <record model="ir.model.access" id="access_lot_group_stock_lot_admin">
            <field name="model" search="[('model', '=', 'stock.lot')]"/>
            <field name="group" ref="group_stock_lot_admin"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <record model="ir.model.access" id="access_lot_group_stock_admin">
            <field name="model" search="[('model', '=', 'stock.lot')]"/>
            <field name="group" ref="stock.group_stock_admin"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>

        <!-- stock.move -->
        <record model="ir.model.access" id="access_move_group_stock_admin">
            <field name="model" search="[('model', '=', 'stock.move')]"/>
            <field name="group" ref="stock.group_stock_admin"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="False"/>
        </record>

        <!-- stock.location -->
        <record model="ir.ui.view" id="location_view_form">
            <field name="model">stock.location</field>
            <field name="inherit" ref="stock.location_view_form"/>
            <field name="name">location_form</field>
        </record>

        <record model="ir.ui.view" id="location_view_list">
            <field name="model">stock.location</field>
            <field name="inherit" ref="stock.location_view_tree"/>
            <field name="name">location_list</field>
        </record>
    </data>
</tryton>
